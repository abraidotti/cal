<h1>Events#new</h1>

<%= form_for @event do |f| %>
<br>
<%= f.label "Name: " %>
<%= f.text_field :name %>
<br>
<%= f.label "Description: " %>
<%= f.text_field :description %>
<br>
<%= f.label "Start time" %>
<%= f.datetime_select :start_time %>
<br>
<%= f.label "End time" %>
<%= f.datetime_select :end_time %>
<br>
<%= f.submit %>
<% end %>
