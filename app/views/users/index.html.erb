<div class="container">
  <div class="row">
    <div class="one-half column" style="margin-top: 1%">
      <h3>All users</h3>
      <ul style="list-style: none;">
        <% @users.each do |user| %>
          <li>
            <a href="/users/show/<%= user.id%>"><%= user.fname %>
              <%= user.lname %></a>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
